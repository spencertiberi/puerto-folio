{
  "extends": ["airbnb"],
  "env": {
    "browser": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "rules": {
    "semi": ["error", "never"],
    "arrow-parens": ["error", "always"],
    "no-plusplus": 0,
    "no-alert": 2,
    "no-console": 2,
    "import/prefer-default-export": 0,
    "react/prop-types": 0,
    "react/jsx-filename-extension": [0],
    "no-confusing-arrow": 0,
    "object-curly-newline": 0,
    "arrow-body-style": 0,
    "implicit-arrow-linebreak": 0
  }
}
